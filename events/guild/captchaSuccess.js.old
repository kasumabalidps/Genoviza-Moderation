const captcha = require("../../utils/captcha");

module.exports = (client) => {
    const captchaInstance = captcha(client);

    captchaInstance.on("success", (data) => {
        const channel = client.channels.cache.get('1256676647223038075');
        if (channel) {
            channel.send(`A Member has Solved a CAPTCHA!`);
            channel.send(`User: ${data.member.user.tag}, UserID: ${data.member.user.id}`);
        }
    });
};
